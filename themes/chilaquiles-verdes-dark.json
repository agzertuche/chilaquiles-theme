  "tokenColors": [
    {
      "name": "Default text",
      "scope": [
        "source",
        "text"
      ],
      "settings": {
        "foreground": "#F5F1E8"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.line",
        "comment.block",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#5C6E5E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords and storage",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.control.flow",
        "keyword.control.import",
        "keyword.control.export",
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#4CAF50",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.arithmetic"
      ],
      "settings": {
        "foreground": "#4CAF50"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.quoted.single",
        "string.quoted.double",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#E8B84B"
      }
    },
    {
      "name": "Template expression punctuation",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#4CAF50"
      }
    },
    {
      "name": "Numbers and constants",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#6FAF5A"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#D4A017"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#F5F1E8"
      }
    },
    {
      "name": "Constants / enum members",
      "scope": [
        "variable.other.constant",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#E8C96A"
      }
    },
    {
      "name": "Types and classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#C97DB5"
      }
    },
    {
      "name": "Type annotations (TS/Rust)",
      "scope": [
        "entity.name.type.primitive",
        "support.type.primitive",
        "keyword.type"
      ],
      "settings": {
        "foreground": "#C97DB5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Object properties / member access",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#E8B84B"
      }
    },
    {
      "name": "Imports / module names",
      "scope": [
        "entity.name.module",
        "support.module",
        "meta.import string"
      ],
      "settings": {
        "foreground": "#E8B84B"
      }
    },
    {
      "name": "Decorators / annotations",
      "scope": [
        "meta.decorator",
        "punctuation.decorator",
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#4CAF50",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML / JSX tags",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#C97DB5"
      }
    },
    {
      "name": "HTML / JSX attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#D99DC8"
      }
    },
    {
      "name": "CSS selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#C97DB5"
      }
    },
    {
      "name": "CSS properties",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#D4A017"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#8AAD8C"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "punctuation.definition.parameters",
        "punctuation.definition.block",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#F5F1E8"
      }
    },
    {
      "name": "Markdown headings",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#4CAF50",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#E8B84B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#F5F1E8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown inline code",
      "scope": [
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#6FAF5A"
      }
    },
    {
      "name": "Markdown links",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#E8B84B",
        "fontStyle": "underline"
      }
    },
    {
      "name": "TS/JS — type assertion angle brackets",
      "scope": [
        "meta.type.assertion.ts",
        "meta.type.assertion.tsx"
      ],
      "settings": {
        "foreground": "#C97DB5"
      }
    },
    {
      "name": "TS/JS — generic type parameters in calls",
      "scope": [
        "meta.type.parameters.ts entity.name.type",
        "meta.type.parameters.tsx entity.name.type"
      ],
      "settings": {
        "foreground": "#C97DB5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "TS/JS — arrow function",
      "scope": [
        "storage.type.function.arrow.ts",
        "storage.type.function.arrow.tsx",
        "storage.type.function.arrow.js"
      ],
      "settings": {
        "foreground": "#4CAF50"
      }
    },
    {
      "name": "TS/JS — interface/type name",
      "scope": [
        "entity.name.type.interface.ts",
        "entity.name.type.alias.ts"
      ],
      "settings": {
        "foreground": "#C97DB5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "TS/JS — built-in types (string, number, boolean)",
      "scope": [
        "support.type.primitive.ts",
        "support.type.primitive.tsx",
        "support.type.primitive.js"
      ],
      "settings": {
        "foreground": "#C97DB5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "TS/JS — import/export keywords",
      "scope": [
        "keyword.control.import.ts",
        "keyword.control.export.ts",
        "keyword.control.import.tsx",
        "keyword.control.export.tsx",
        "keyword.control.import.js",
        "keyword.control.export.js"
      ],
      "settings": {
        "foreground": "#4CAF50",
        "fontStyle": "bold"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#E8B84B",
    "type": "#C97DB5",
